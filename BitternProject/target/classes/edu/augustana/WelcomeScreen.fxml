<VBox id="myVBox" alignment="CENTER" prefHeight="370.0" prefWidth="455.0" spacing="20.0" style="-fx-background-image: url(&quot;https://i.pinimg.com/564x/6f/b9/83/6fb9838f2d2c24b7b00a57fb4dc62317.jpg&quot;); -fx-background-repeat: no-repeat; -fx-background-size: cover;" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.augustana.WelcomeController">
   <children>
      <Label text="HAM Radio Simulator">
         <font>
            <Font name="Apple Braille" size="36.0" />
         </font></Label>
      <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="25.0">
         <children>
            <Button fx:id="trainingButton" onAction="#switchToTraining" text="Training" />
            <Button fx:id="scenarioMenuButton" onAction="#switchToScenario" text="Scenarios" />
            <Button fx:id="serverButton" onAction="#switchToServer" text="Server" />
         </children>
      </HBox>
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
