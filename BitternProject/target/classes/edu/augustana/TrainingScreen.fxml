<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="417.0" prefWidth="528.0" spacing="20.0" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.augustana.TrainingController">
    <children>
      <HBox alignment="CENTER" prefHeight="30.0" prefWidth="452.0" spacing="50.0">
         <children>
              <Button fx:id="welcomeButton" alignment="TOP_CENTER" onAction="#switchToWelcome" text="Return to Homescreen">
               <font>
                  <Font size="10.0" />
               </font>
            </Button>
            <CheckBox fx:id="phrasesCheckbox" mnemonicParsing="false" text="Include CW phrases" />
         </children>
      </HBox>
        <Label alignment="CENTER" contentDisplay="CENTER" graphicTextGap="0.0" prefHeight="42.0" prefWidth="133.0" text="Training" textAlignment="CENTER">
         <font>
            <Font name="Rockwell" size="24.0" />
         </font>
         <VBox.margin>
            <Insets />
         </VBox.margin></Label>
      <TextArea prefHeight="232.0" prefWidth="452.0" />
    </children>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
</VBox>
